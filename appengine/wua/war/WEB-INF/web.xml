<web-app xmlns="http://java.sun.com/xml/ns/javaee" version="2.5">
  <welcome-file-list>
    <welcome-file>welcome.jsp</welcome-file></welcome-file-list>
  <servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>wua.Login</servlet-class></servlet>
  <servlet>
    <servlet-name>updaterpc</servlet-name>
    <servlet-class>wua.UpdateRPC</servlet-class></servlet>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login</url-pattern></servlet-mapping>
  <servlet-mapping>
    <servlet-name>updaterpc</servlet-name>
    <url-pattern>/api</url-pattern></servlet-mapping>
  <servlet>
  	<servlet-name>migrate</servlet-name>
  	<servlet-class>wua.Migrate</servlet-class></servlet>
  <servlet-mapping>
  	<servlet-name>migrate</servlet-name>
  	<url-pattern>/admin/migrate</url-pattern></servlet-mapping>
  <security-constraint>
    <web-resource-collection>
      <url-pattern>/admin/*</url-pattern>
    </web-resource-collection>
    <auth-constraint>
      <role-name>admin</role-name>
    </auth-constraint></security-constraint>
</web-app>