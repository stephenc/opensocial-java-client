<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app xmlns="http://java.sun.com/xml/ns/javaee" version="2.5">
	<servlet>
		<servlet-name>giftscontroller</servlet-name>
		<servlet-class>swt.controller.GiftsController</servlet-class>
	</servlet>	
	<servlet-mapping>
		<servlet-name>giftscontroller</servlet-name>
		<url-pattern>/giftscontroller</url-pattern>
	</servlet-mapping>	

	<servlet>
		<servlet-name>logincontroller</servlet-name>
		<servlet-class>swt.controller.LoginController</servlet-class>
	</servlet>	
	<servlet-mapping>
		<servlet-name>logincontroller</servlet-name>
		<url-pattern>/logincontroller</url-pattern>
	</servlet-mapping>	
	
	
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
